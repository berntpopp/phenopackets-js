{
  "name": "phenopackets-js",
  "version": "0.1.0",
  "description": "JavaScript/TypeScript library for GA4GH Phenopackets, generated from protobuf definitions.",
  "main": "index.js",
  "scripts": {
    "download-protos": "node -e \"process.platform === 'win32' ? require('child_process').execSync('scripts\\\\download-protos.bat', {stdio: 'inherit'}) : require('child_process').execSync('bash ./scripts/download-protos.sh', {stdio: 'inherit'})\"",
    "generate-protos": "bash ./scripts/generate-protos.sh",
    "setup": "npm run download-protos && npm run generate-protos",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ga4gh",
    "phenopackets",
    "phenotype",
    "genomics",
    "clinical",
    "protobuf",
    "vrs",
    "vrsatile"
  ],
  "author": "Bernt Popp <bernt.popp@gmail.com>",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "google-protobuf": "^3.21.2",
    "grpc-tools": "^1.12.4"
  }
}
